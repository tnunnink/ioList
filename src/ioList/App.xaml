<prism:PrismApplication x:Class="ioList.App"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:prism="http://prismlibrary.com/">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Resources/Brushes.xaml" />
        <ResourceDictionary Source="Resources/Styles/ButtonStyle.xaml" />
        <ResourceDictionary Source="Resources/Styles/ListViewItemStyle.xaml" />
        <ResourceDictionary Source="Resources/Styles/IconButtonStyle.xaml" />
        <ResourceDictionary Source="Resources/Styles/WindowStyle.xaml"/>
        <ResourceDictionary Source="Resources/Styles/DialogStyle.xaml" />
        <ResourceDictionary Source="Resources/Icons/GenericIcons.xaml" />
        <ResourceDictionary Source="Resources/Icons/WindowsIcons.xaml" />
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</prism:PrismApplication>
