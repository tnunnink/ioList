<prism:PrismApplication x:Class="ioList.App"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:prism="http://prismlibrary.com/">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/CoreWPF;component/resources.xaml" />
        <ResourceDictionary Source="Resources/Brushes.xaml" />
        <ResourceDictionary Source="Resources/Icons.xaml" />
        <ResourceDictionary Source="Resources/Styles/WindowStyle.xaml" />
        <ResourceDictionary Source="Resources/Styles/IconStyle.xaml" />
        <ResourceDictionary Source="Resources/Styles/ButtonStyle.xaml" />
        <ResourceDictionary Source="Resources/Styles/PopupButtonStyle.xaml" />
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</prism:PrismApplication>